{
    "name": "gmo/common",
    "license": "MIT",
    "description": "A library for common code",
    "require": {
        "firebase/php-jwt": "~1.0",
        "nesbot/carbon": "^1.20",
        "php": ">=5.3.3",
        "seld/jsonlint": "^1.4",
        "symfony/http-foundation": "^2.6 || ^3.0",
        "symfony/polyfill-php55": "^1.0",
        "symfony/polyfill-php71": "^1.0",
        "symfony/yaml": "^2.5 || ^3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "bolt/collection": "^1.0",
        "gmo/cache": "~1.0",
        "monolog/monolog": "^1.18",
        "phpunit/phpunit": "^4.8",
        "predis/predis": "^1.0",
        "symfony/cache": "^3.0",
        "psr/log": "^1.0",
        "silex/silex": "1.3.*@dev",
        "symfony/phpunit-bridge": "^2.8 || ^3.0",
        "twig/twig": "~1.8"
    },
    "suggest": {
        "psr/log": "Provides a basic console logger for PSR-3 interface",
        "monolog/monolog": "Provides serializable formatter"
    },
    "autoload": {
        "psr-4": {
            "Gmo\\Common\\": "src",
            "GMO\\Common\\": "src/",
            "GMO\\TestHelper\\": "TestHelper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmo\\Common\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
