{
	"name": "gmo/common",
	"license": "MIT",
	"description": "A library for common code",
	"require": {
		"php": ">=5.3.3",
		"symfony/yaml": "^2.5 || ^3.0",
		"symfony/http-foundation": "^2.6 || ^3.0",
		"firebase/php-jwt": "~1.0"
	},
	"require-dev": {
		"gmo/cache": "~1.0",
		"psr/log": "1.0.0",
		"monolog/monolog": "^1.18",
		"nesbot/carbon": "^1.20",
		"silex/silex": "1.3.*@dev",
		"twig/twig": "~1.8",
		"phpunit/phpunit": "3.7.*"
	},
	"suggest": {
		"psr/log": "Provides a basic console logger for PSR-3 interface",
		"monolog/monolog": "Provides serializable formatter",
		"silex/silex": "Provides twig responses",
		"twig/twig": "Provides twig responses"
	},
	"autoload": {
		"psr-4": {
			"GMO\\Common\\": "src/",
			"GMO\\TestHelper\\": "TestHelper/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
